{
  "context": {
    "fileName": "AGENTS.md"
  },
  "mcpServers": {
    "exa": {
      "httpUrl": "https://mcp.exa.ai/mcp",
      "headers": {}
    },
    "chrome-devtools": {
      "command": "bunx",
      "args": [
        "chrome-devtools-mcp@latest"
      ]
    },
    "context7": {
      "command": "bunx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ]
    },
    "sequential_thinking": {
      "command": "bunx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    }
  },
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  }
}