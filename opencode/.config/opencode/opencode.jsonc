  "mcp": {
    "chrome-devtools": {
      "type": "local",
      "command": ["bunx", "chrome-devtools-mcp@latest"],
      "enabled": true
    },
    "context7": {
      "type": "local",
      "command": ["bunx", "-y", "@upstash/context7-mcp"],
      "enabled": true
    },
    "sequential-thinking": {
      "type": "local",
      "command": ["bunx", "-y", "@modelcontextprotocol/server-sequential-thinking"],
      "enabled": true
    },
    "exa": {
      "type": "local",
      "command": ["bunx", "-y", "exa-mcp-server", "tools=get_code_context_exa,web_search_exa"],
      "environment": {
        "EXA_API_KEY": "{env:EXA_API_KEY}"
      },
      "enabled": true
    },
    "grep": {
      "type": "remote",
      "url": "https://mcp.grep.app",
      "enabled": true
    }
  }
}
