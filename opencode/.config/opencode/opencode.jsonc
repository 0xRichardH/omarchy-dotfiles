{
  "$schema": "https://opencode.ai/config.json",
  "instructions": ["CONTRIBUTING.md", "docs/guidelines.md", ".cursor/rules/*.md"],
  "theme": "system",
  "autoupdate": true,
  "small_model": "opencode/grok-code",
  "plugin": [
    "opencode-google-antigravity-auth@latest", // https://github.com/shekohex/opencode-google-antigravity-auth
    "opencode-openai-codex-auth@latest", // https://github.com/numman-ali/opencode-openai-codex-auth
    "opencode-supermemory@latest", // https://github.com/supermemoryai/opencode-supermemory
  ],
  "keybinds": {
    "leader": "ctrl+x"
  },
  "provider": {
    "google": {
      "models": {
        "gemini-3-pro-preview": {
          "options": {
            "thinkingConfig": {
              "thinkingLevel": "high",
              "includeThoughts": true
            }
          }
        },
        "gemini-3-flash": {
          "options": {
            "thinkingConfig": {
              "thinkingLevel": "medium",
              "includeThoughts": true
            }
          }
        },
        "gemini-2.5-flash": {
          "options": {
            "thinkingConfig": {
              "thinkingBudget": 8192,
              "includeThoughts": true
            }
          }
        },
        "gemini-claude-opus-4-5-thinking": {
          "options": {
            "thinkingConfig": {
              "thinkingBudget": 32000,
              "includeThoughts": true
            }
          }
        },
        "gemini-claude-sonnet-4-5-thinking": {
          "options": {
            "thinkingConfig": {
              "thinkingBudget": 16000,
              "includeThoughts": true
            }
          }
        }
      }
    },
    "openai": {
      "options": {
        "reasoningEffort": "medium",
        "reasoningSummary": "auto",
        "textVerbosity": "medium",
        "include": [
          "reasoning.encrypted_content"
        ],
        "store": false
      },
      "models": {
        "gpt-5.2-codex-low": {
          "name": "GPT 5.2 Codex Low (OAuth)",
